<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="MyQiita.View.MainPage"
            xmlns:myqiita="clr-namespace:MyQiita.View"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
            NavigationPage.HasNavigationBar="False"
            android:TabbedPage.ToolbarPlacement="Bottom"
            >
  <!--Pages can be added as references or inline-->
    <myqiita:HomePage Title="Home" IconImageSource="home.png"></myqiita:HomePage>
    <myqiita:ItemListPage Title="Search" IconImageSource="search.png"></myqiita:ItemListPage>
    <myqiita:SettingPage Title="Setting" IconImageSource="setting.png"></myqiita:SettingPage>
</TabbedPage>
